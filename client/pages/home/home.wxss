/* client/pages/home/home.wxss */

page {
  height: 100%;
}

.home {
  display: flex;
  height: 100%;
  flex-direction: column;
  align-items: center;
  background-color: #e8b1dd;
}

.home image {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 250rpx;
  height: 250rpx;
  transform: translate(-50%, -50%);
}

.home .van-button--plain.van-button--danger {
  border: 1px solid #57a6f3;
  color: #57a6f3;
}
